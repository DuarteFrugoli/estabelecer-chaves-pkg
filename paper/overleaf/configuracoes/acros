%\acsetup{foreign-format = {\emph}}
% 

%%% Numbers

\DeclareAcronym{5G}{
	short = 5G,
	long  = Quinta Geração de Rede Móvel Celular,
}

\DeclareAcronym{6G}{
	short = 6G,
	long  = Sexta Geração de Rede Móvel Celular,
}

%%% A
\DeclareAcronym{AC}{
	short = AC,
	long  = Autentidade certificadora,
	foreign = {\emph{Certification Authority}},
}

\DeclareAcronym{AES}{
	short = AES,
	long  = {\emph{Advanced Encryption Standard}},
}

\DeclareAcronym{AWGN}{
	short = AWGN,
	long  = {Ruído Gaussiano Branco Aditivo},
    foreign = {\emph{Additive white Gaussian noise}},
}


%%% B
\DeclareAcronym{B5G}{
	short = B5G,
	long  = 5G a Além,
	foreign = {\emph{5G and Beyond}},
}

\DeclareAcronym{BER}{
	short = BER,
	long  = Taxa de Erro de Bit,
	foreign = {\emph{Bit Error Rate}},
}

\DeclareAcronym{BPSK}{
	short = BPSK,
	long  = Chaveamento de Fase Binário,
	foreign = {\emph{Binary Phase Shift Keying}},
}

\DeclareAcronym{BCH}{
	short = BCH,
	long  = Bose-Chaudhuri-Hocquenghem,
}


%%% C
\DeclareAcronym{CIA}{
	short = CIA,
	long  = Confidencialidade Integridade Disponibilidade,
	foreign = {\emph{Confidentiality Integrity Availability}},
}

%%% D
\DeclareAcronym{DDoS}{
	short = DDoS,
	long  = Ataque Distribuído de Negação de Serviço,
	foreign = {\emph{Distributed Denial of Service}},
}

%%% E
\DeclareAcronym{ECDH}{
	short = ECDH,
	long  = {\emph{Elliptic Curve Diffie-Hellman}},
}

%%% F
\DeclareAcronym{FDP}{
	short = FDP,
	long  = Função Densidade de Probabilidade,
}

%%% G


%%% H


%%% I
\DeclareAcronym{IPSec}{
	short = IPSec,
	long  = Segurança de protocolo de internet,
	foreign = {\emph{Internet Protocol Security}},
}

\DeclareAcronym{IKE}{
	short = IKE,
	long  = Troca de chaves pela internet,
	foreign = {\emph{Internet Key Exchange}},
}
\DeclareAcronym{IoT}{
	short = IoT,
	long  = Internet das Coisas,
	foreign = {\emph{Internet of Things}},
}
\DeclareAcronym{ICV}{
	short = ICV,
	long  = Valor de confirmação de integridade,
	foreign = {\emph{Integrity check value}},
}



%%% J


%%% K
\DeclareAcronym{KDR}{
	short = KDR,
	long  = Taxa de Discordância de Chaves,
	foreign = {\emph{Key Disagreement Rate}},
}


%%% L
\DeclareAcronym{LDPC}{
	short = LDPC,
	long  = Low-Density Parity-Check,
}


%%% M
\DeclareAcronym{MIMO}{
	short = MIMO,
	long  = Múltiplas Entradas Múltiplas Saídas,
	foreign = {\emph{Multiple-Input Multiple-Output}},
}


%%% N
\DeclareAcronym{NIST}{
	short = NIST,
	long  = Instituto Nacional de Padrões e Tecnologia,
	foreign = {\emph{National Institute of Standards and Technology}},
}

\DeclareAcronym{NLoS}{
	short = NLoS,
	long  = Sem Linha de Visada,
	foreign = {\emph{Non-Line-of-Sight}},
}

%%% O


%%% P

\DeclareAcronym{PKG}{
	short = PKG,
	long  = Geração de Chaves em Camada Física,
	foreign = {\emph{Physical Layer Key Generation}},
}

\DeclareAcronym{PKI}{
	short = PKI,
	long  = Infraestrutura de chave pública,
	foreign = {\emph{Public Key Infrastructure}},
}

\DeclareAcronym{PLS}{
	short = PLS,
	long  = Segurança da Camada Física,
	foreign = {\emph{Physical Layer Security}},
}

%%% Q
\DeclareAcronym{QKD}{
    short = QKD,
    long = Distribuição quântica de chaves,
    foreign = {\emph{Quantum Key Distribution}},
}

\DeclareAcronym{QPSK}{
	short = QPSK,
	long  = Chaveamento de Fase Quadratura,
	foreign = {\emph{Quadrature Phase Shift Keying}},
}

%%% R
\DeclareAcronym{RSSI}{
	short = RSSI,
	long  = Indicador de Força de Sinal Recebido,
	foreign = {\emph{Received Signal Strength Indicator}},
}

\DeclareAcronym{RSA}{
	short = RSA,
	long  = {\emph{Rivest-Shamir-Adleman}},
}


%%% S
\DeclareAcronym{SHA}{
	short = SHA,
	long  = Algoritmo de Hash Seguro,
	foreign = {\emph{Secure Hash Algorithm}},
}

\DeclareAcronym{SNR}{
	short = SNR,
	long  = Relação Sinal-Ruído,
	foreign = {\emph{Signal-to-Noise Ratio}},
}


%%% T
\DeclareAcronym{TLS}{
	short = TLS,
	long  = Segurança da camada de transporte,
	foreign = {\emph{Transport Layer Security}},
}

